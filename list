  {
    "id": "16",
    "name": "Salted Caramel Brownies",
    "image": "https://images.unsplash.com/photo-1622732795328-01728a444a11",
    "author": "ChocoholicMike",
    "prepTime": "25 mins",
    "cookTime": "35 mins",
    "servings": 16,
    "difficulty": "Medium",
    "ingredients": [
      "1 cup unsalted butter",
      "8 oz dark chocolate (70%)",
      "1 1/4 cups granulated sugar",
      "3/4 cup packed brown sugar",
      "4 large eggs",
      "1 tbsp vanilla extract",
      "1 cup all-purpose flour",
      "1/2 cup cocoa powder",
      "1 tsp salt",
      "1 cup caramel sauce",
      "Flaky sea salt for topping"
    ],
    "instructions": [
      "Preheat oven to 350°F (175°C). Line 9x13 pan with parchment.",
      "Melt butter and chocolate in double boiler; cool slightly.",
      "Whisk in sugars, then eggs one at a time, then vanilla.",
      "Sift in flour, cocoa and salt; fold until just combined.",
      "Pour 2/3 batter into pan; drizzle with 1/2 cup caramel sauce.",
      "Add remaining batter; swirl with knife.",
      "Bake 30-35 minutes until toothpick comes out with moist crumbs.",
      "Drizzle remaining caramel; sprinkle with sea salt.",
      "Cool completely before cutting."
    ],
    "tips": "For cleaner cuts, chill brownies 2 hours before slicing with hot knife."
  }
