    "id": "2",
    "name": "French Macarons",
    "image": "https://images.unsplash.com/photo-1558326567-98ae2405596b",
    "author": "ParisPatisserie",
    "prepTime": "45 mins",
    "cookTime": "15 mins",
    "servings": 20,
    "difficulty": "Advanced",
    "ingredients": [
      "1 3/4 cups powdered sugar",
      "1 cup almond flour",
      "3 large egg whites",
      "1/4 cup granulated sugar",
      "1/2 tsp vanilla extract",
      "Gel food coloring (optional)",
      "1/2 cup buttercream or ganache (filling)"
    ],
    "instructions": [
      "Sift powdered sugar and almond flour together twice.",
      "Beat egg whites until foamy, then gradually add sugar.",
      "Whip to stiff peaks (about 8 minutes).",
      "Gently fold dry ingredients into meringue in thirds.",
      "Pipe 1-inch circles onto parchment-lined trays.",
      "Let rest 30-60 minutes until dry to the touch.",
      "Bake at 300°F (150°C) for 15 minutes.",
      "Cool completely before filling."
    ],
    "tips": "Age egg whites at room temperature for 24 hours for better texture."
